#include <cmath>
#include <iostream>
#include <string>
using namespace std;
const int N=100;

int b(int n){
    return n*3;
}

int a(int n){
    int ostatnia = n%10;
    n/=10;
    int przedostatnia=n%10;
    return (n + (ostatnia - przedostatnia))*10 + przedostatnia;
}

int c(int n){
    int copy=0;
    int p=1;
    while(n>9){
        copy+=(n%10)*p;
        p*=10;
        n/=10;
    }
    return copy;
}

bool sek(int n1, int n2, string &s, int i){
    if(i==7){
        return false;
    }
    if(n1==n2){
        return true;
    }
    s+="B";
    if(sek(b(n1),n2,s,i+1)) return true;
    if(n1>9){
        s[s.size()-1]='A';
        if(sek(a(n1),n2,s,i+1)) return true;
        s[s.size()-1]='C';
        if(sek(c(n1),n2,s,i+1)) return true;
    }
    if(i>0)
        s = s.substr(0,s.size()-1);
    return false;
}


string supSek(int n1, int n2){
    string s="";
    sek(n1,n2,s,0);
    return s;
}

int main()
{
    cout<<supSek(6,3);
}
