#include <cmath>
#include <iostream>
#include <string>
using namespace std;
const int N=100;

bool waga(string s1, string s2){
    int suma1=0,suma2=0;
    for(int i=0;i<s1.size();i++)
        suma1+=s1[i];
    for(int i=0;i<s2.size();i++)
        suma2+=s2[i];
        
    return suma1==suma2;
}

string przeslon(string s, int j,int size){
    string newString = "";
    for(int i=0;i<j;i++)
        newString= newString+s[i];
    
    for(int i=j+1;i<size;i++)
        newString = newString+s[i];
    return newString;
}

bool wyraz(string s1, string s2, int i,const int size){
    if(i==size)
        return false;
    if(waga(s1,s2)){
        cout<<s1<<endl<<s2<<endl;
        return true;
    }
    return wyraz(przeslon(s1,i,size),s2,i+1,size) || wyraz(s1,s2,i+1,size);
}

int main()
{
    string s1, s2;
    cin>>s1>>s2;
    cout<< wyraz(s1,s2,0,s1.size());
}
